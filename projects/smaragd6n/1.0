#%Module1.0# # -*- tcl -*-

module-whatis	"Sets up static project configuration for Smaragd 1.0"

setenv PROJECT smaragd
#
setenv FOUNDRY_EIC TSMC
setenv FOUNDRY_PIC imec_nhanced
#
setenv CHIP_REV_EIC 1p0
setenv CHIP_REV_PIC 1p0
#
setenv PDK_PROCESS_EIC tsmcN12
setenv PDK_PROCESS_PIC n130
#
setenv PROCESS_EIC         tsmcn12
setenv PROCESS_VARIANT_EIC hpcp
setenv METAL_STACK_EIC     13M_2Xa1Xd3Xe2Y2Yy2R
setenv PROCESS_VARIANT_EIC sp
setenv PROJECT_CDS_LIB_EIC ""
#
setenv PROCESS_PIC         n130
setenv METAL_STACK_PIC     1P2M_2Z
setenv PROJECT_CDS_LIB_PIC ""
#
setenv SOS_SERVER_EIC uc_tsmcn12
setenv SOS_SERVER_PIC uc_photonics
#
prepend-path PATH /data/tools/lt/pd_tools/bin


# Quality of Life Cadence Updates
setenv W3264_NOWARN_UNSUPPORTED_CENTOS 1
setenv CDS_AUTO_64BIT ALL


