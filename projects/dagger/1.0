#%Module1.0# # -*- tcl -*-

module-whatis	"Sets up static project configuration for Dagger 1.0"

setenv PROJECT dagger
#
setenv FOUNDRY_EIC TSMC
setenv FOUNDRY_PIC IMEC
#
setenv CHIP_REV_EIC p1.0
setenv CHIP_REV_PIC p1.0
#
setenv PDK_PROCESS_EIC tsmcN28
setenv PDK_PROCESS_PIC imec130
#
setenv PROCESS_EIC         tsmcn28
setenv PROCESS_VARIANT_EIC hpcp
setenv METAL_STACK_EIC     1P10M_5X2Y2Z
setenv PROJECT_CDS_LIB_EIC /data/proj/mosaic/eic/$env(CHIP_REV_EIC)/pd/cds/cds.lib
#
setenv PROCESS_PIC         imec130
setenv PROCESS_VARIANT_EIC sp
setenv METAL_STACK_PIC     1P2M_2Z
setenv PROJECT_CDS_LIB_PIC /data/proj/dagger/pic/$env(CHIP_REV_PIC)/pd/cds/cds.lib
#
prepend-path PATH /data/proj/mosaic/eic/$env(CHIP_REV_EIC)/pd/scripts/bin

prepend-path PATH /data/tools/lt/pd_tools/bin

# Add per checkout directories to path
# export PATH="$PROJ_DIR/scripts:$PATH"
# export PATH="$PROJ_DIR/digital/dv/scripts:$PATH"
# export PATH="$PROJ_DIR/digital/rtl/scripts:$PATH"
# export PATH="$PROJ_DIR/digital/pd/scripts:$PATH"
# export PATH="$PROJ_DIR/digital/pd/alpha_mosaic/scripts/oa_common_ui/synth:$PATH"
# export PATH="$PROJ_DIR/analog/scripts:$PATH"

# Quality of Life Cadence Updates
setenv W3264_NOWARN_UNSUPPORTED_CENTOS 1
setenv CDS_AUTO_64BIT ALL

# Define drc runset area
setenv ISIPP_DRC_INCLUDE_DIR /data/tools/lt/pd_techdata/imec130sp/runset/
