#%Module1.0# # -*- tcl -*-

module-whatis	"Sets up static project configuration for Aurora FC 1.0"

setenv PROJECT aurora_fc
#
setenv FOUNDRY_EIC TSMC
setenv FOUNDRY_PIC IMEC_NHANCED
#
setenv CHIP_REV_EIC 1p0
setenv CHIP_REV_PIC 1p0
#
setenv PDK_PROCESS_EIC tsmcN6
setenv PDK_PROCESS_PIC imec130
#
setenv PROCESS_EIC         tsmcn12
setenv METAL_STACK_EIC     13M_2Xa1Xd3Xe2Y2Yy2R
setenv PROJECT_CDS_LIB_EIC ""
setenv PROCESS_VARIANT_EIC sp
#
setenv PROCESS_PIC         ""
setenv METAL_STACK_PIC     ""
setenv PROJECT_CDS_LIB_PIC ""
#
setenv SOS_SERVER_EIC uc_tsmc12
setenv SOS_SERVER_PIC ""
