#%Module1.0# # -*- tcl -*-

module-whatis	"Sets up static project configuration for Tahoma 1.0"

setenv PROJECT tahoma_1.0

# Enables per project PIC Physical Verification
setenv TECH_NAME_PIC tsmcN65
setenv PROCESS_PIC tsmcn65
setenv FOUNDRY_PIC TSMC
setenv METAL_STACK_PIC 1P6M_3X2Z

# Used to enable automatic update of Black Box LVS
setenc LT_PDK_VERSION tsmc_ltpdk_V0
setenv PROJECT_CDS_LIB_PIC /data/proj/$env(PROJECT)/pd/cds/siph_cds.lib

# Enables per project EIC Physical Verification
setenv TECH_NAME tsmcN28
setenv FOUNDRY TSMC
setenv PROCESS tsmcn28
setenv VARIANT hpcp
setenv METAL_STACK 1P10M_5X2Y2Z
setenv PROJECT_CDS_LIB /data/proj/$env(PROJECT)/pd/cds/cds.lib

prepend-path PATH /data/proj/$env(PROJECT)/pd/scripts/bin

setenv W3264_NOWARN_UNSUPPORTED_CENTOS 1
