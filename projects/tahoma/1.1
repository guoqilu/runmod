#%Module1.0# # -*- tcl -*-

module-whatis	"Sets up static project configuration for Tahoma 1.1"

setenv PROJECT tahoma
#
setenv FOUNDRY_EIC TSMC
setenv FOUNDRY_PIC TSMC
#
setenv CHIP_REV_EIC p1.0
setenv CHIP_REV_PIC p1.0
#
setenv PDK_PROCESS_EIC tsmcN28
setenv PDK_PROCESS_PIC tsmnN65
#
setenv PROCESS_EIC         tsmcn28
setenv PROCESS_VARIANT_EIC hpcp
setenv METAL_STACK_EIC     1P10M_5X2Y2Z
setenv PROJECT_CDS_LIB_EIC /data/proj/tahoma/eic/$env(CHIP_REV_EIC)/pd/cds/cds.lib
#
setenv PROCESS_PIC         tsmcn65
setenv METAL_STACK_PIC     1P6M_3X2Z
setenv PROJECT_CDS_LIB_PIC /data/proj/tahoma/pic/$env(CHIP_REV_PIC)/pd/cds/cds.lib
#
prepend-path PATH /data/proj/tahoma/eic/$env(CHIP_REV_EIC)/pd/scripts/bin

prepend-path PATH /data/tools/lt/pd_tools/bin

# Quality of Life Cadence Updates
setenv W3264_NOWARN_UNSUPPORTED_CENTOS 1
setenv CDS_AUTO_64BIT ALL
