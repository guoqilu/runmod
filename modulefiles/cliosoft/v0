#%Module1.0# # -*- tcl -*-

module load toolbase

module-whatis	"Cliosoft SOS"

setenv CLIOSOFT_DIR $env(TOOLBASE)/cliosoft/sos_7.05.p7_linux64
prepend-path PATH $env(CLIOSOFT_DIR)/bin
setenv GDM_USE_SHLIB_ENVVAR 1
prepend-path LD_LIBRARY_PATH $env(CLIOSOFT_DIR)/lib
prepend-path LD_LIBRARY_PATH $env(CLIOSOFT_DIR)/lib/64bit
setenv CLIOLMD_LICENSE_FILE 21541@10.200.4.8