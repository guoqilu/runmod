#%Module1.0# # -*- tcl -*-

module-whatis	"Loads allegro paths"

module load cadence

set	allegro_version 174
setenv ALLEGROHOME $env(CADENCE_DIR)/SPB$allegro_version

prepend-path    PATH	$env(ALLEGROHOME)/tools/dfII/bin
prepend-path    PATH	$env(ALLEGROHOME)/tools/bin
prepend-path    CDNSHELP_DOC $env(ALLEGROHOME)/doc
