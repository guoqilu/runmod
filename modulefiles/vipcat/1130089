#%Module1.0# # -*- tcl -*-

module-whatis	"Load VIPCAT paths"

module load cadence/v1

set vipcat_version 1130089
set XCELIUM_VERS [file tail $env(XCELIUMHOME)]

setenv VIPCATHOME $env(CADENCE_DIR)/VIPCAT$vipcat_version
setenv DENALI     $env(VIPCATHOME)/tools/denali_64bit
setenv CDN_VIP_ROOT $env(VIPCATHOME)
setenv CDS_INST_DIR $env(XCELIUMHOME)
setenv CDS_ARCH lnx86
setenv CDN_VIP_LIB_PATH $env(VIPCATHOME)/$XCELIUM_VERS/vip_lib

prepend-path    SPECMAN_PATH $env(CDN_VIP_ROOT)/packages
prepend-path    SPECMAN_PATH $env(CDN_VIP_LIB_PATH)/64bit
prepend-path    PATH	$env(VIPCATHOME)/tools.$env(CDS_ARCH)/bin
prepend-path    PATH	$env(VIPCATHOME)/tools/bin/64bit
prepend-path    PATH	$env(VIPCATHOME)/tools/bin
prepend-path    CDNSHELP_DOC $env(VIPCATHOME)/doc
prepend-path    LD_LIBRARY_PATH $env(CDN_VIP_LIB_PATH)/64bit
prepend-path    LD_LIBRARY_PATH $env(DENALI)/verilog
prepend-path    LD_LIBRARY_PATH $env(CDS_INST_DIR)/tools.$env(CDS_ARCH)/specman/lib/64bit
prepend-path    LD_LIBRARY_PATH $env(CDS_INST_DIR)/tools.$env(CDS_ARCH)/lib/64bit
