#%Module1.0# # -*- tcl -*-

module-whatis	"Loads innovus paths"


module load cadence/v1



set	innovus_version 2113
setenv INNOVUSHOME $env(CADENCE_DIR)/INNOVUS$innovus_version
setenv INNOVUS_PATH $env(INNOVUSHOME)/tools.lnx86/bin

prepend-path    PATH	$env(INNOVUSHOME)/tools/dfII/bin
prepend-path    PATH	$env(INNOVUSHOME)/tools/bin
prepend-path    CDNSHELP_DOC $env(INNOVUSHOME)/doc
