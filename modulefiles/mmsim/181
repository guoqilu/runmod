#%Module1.0# # -*- tcl -*-

module-whatis	"Loads mmsim paths"


module load cadence



set	mmsim_version 181
setenv MMSIMHOME $env(CADENCE_DIR)/SPECTRE$mmsim_version

prepend-path    PATH	$env(MMSIMHOME)/tools/dfII/bin
prepend-path    PATH	$env(MMSIMHOME)/tools/bin
prepend-path    CDNSHELP_DOC $env(MMSIMHOME)/doc

prepend-path    LD_LIBRARY_PATH $env(MMSIMHOME)/tools/inca/lib
prepend-path    LD_LIBRARY_PATH $env(MMSIMHOME)/tools/lib
prepend-path    LD_LIBRARY_PATH $env(MMSIMHOME)/tools/ultrasim/lib
