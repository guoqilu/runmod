#%Module1.0# # -*- tcl -*-

module-whatis	"Loads assura paths"

module load cadence

set	assura_version 618_0416109
setenv ASSURAHOME $env(CADENCE_DIR)/ASSURA$assura_version

prepend-path    PATH	$env(ASSURAHOME)/tools/dfII/bin
prepend-path    PATH	$env(ASSURAHOME)/tools/bin
prepend-path    CDNSHELP_DOC $env(ASSURAHOME)/doc
