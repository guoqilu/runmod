#%Module1.0# # -*- tcl -*-

module-whatis	"Loads modus paths"


module load cadence



set	modus_version 181
setenv MODUSHOME $env(CADENCE_DIR)/MODUS$modus_version

prepend-path    PATH	$env(MODUSHOME)/tools/dfII/bin/64bit
prepend-path    PATH	$env(MODUSHOME)/tools/dfII/bin
prepend-path    CDNSHELP_DOC $env(MODUSHOME)/doc
