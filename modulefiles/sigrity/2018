#%Module1.0# # -*- tcl -*-

module-whatis	"Loads sigrity paths"


module load cadence



set	sigrity_version 2018
setenv SIGRITYHOME $env(CADENCE_DIR)/SIGRITY$sigrity_version

prepend-path    PATH	$env(SIGRITYHOME)/tools/bin
prepend-path    PATH	$env(SIGRITYHOME)/tools/dfII/bin
prepend-path    CDNSHELP_DOC $env(SIGRITYHOME)/doc
