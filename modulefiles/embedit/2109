#%Module1.0# # -*- tcl -*-

module-whatis	"Loads embedit paths"


module load synopsys

# license for TSMC N6 SRAM compilers
prepend-path LM_LICENSE_FILE 27020@faraday-1

set	embedit_version S-2021.09
setenv EMBEDITHOME $env(SYNOPSYS_DIR)/embedit/$embedit_version

prepend-path    PATH	$env(EMBEDITHOME)/bin
